package com.ht.persistence.model.datum.bookinfo;

import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import com.ht.common.export.ExcelAttribute;
import com.ht.persistence.model.base.BaseModel;

/**
 * 图书资料实体类
 * 
 * @author zyd
 *
 */
@Entity
@Table(name = "BOOK_INFO")
public class BookInfo extends BaseModel {

	@Id
	@Column(name = "id", length = 64)
	private String id; // id标识
	@ExcelAttribute(name = "图书编号", isMark = true)
	private String code; // 编 号
	@Column(name = "book_name", length = 100)
	@ExcelAttribute(name = "图书名称", isMark = true)
	private String bookName; // 图书名称

	@Column(name = "one_subclass", length = 64)
	@ExcelAttribute(name = "一级子类", isMark = true)
	private String oneSubClass; // 一级子类

	@Column(name = "two_subclass", length = 64)
	@ExcelAttribute(name = "二级子类", isMark = true)
	private String twoSubClass; // 二级子类

	@Column(name = "inventory_num", length = 32)
	@ExcelAttribute(name = "库存数量", isMark = true)
	private String inventoryNum; // 库存数量

	@Column(name = "publish_date")
	@ExcelAttribute(name = "出版年月")
	private Date publishDate; // 出版年月

	@Column(name = "version", length = 10)
	@ExcelAttribute(name = "版本号")
	private String version; // 版本号

	@Column(name = "publish_unit", length = 100)
	@ExcelAttribute(name = "出版单位")
	private String publishUnit; // 出版单位

	@Column(name = "state", length = 8)
	private String state; // 状态(在库，借出)

	@Column(name = "entry", length = 32)
	private String entry; // 录入者

	@Column(name = "reviewers", length = 32)
	private String reviewers; // 审核者
	@ExcelAttribute(name = "作者")
	private String author; // 作 者
	@ExcelAttribute(name = "印刷年月")
	private Date printDate; // 印刷年月
	@ExcelAttribute(name = "书号")
	private String bookNo; // 书 号
	@ExcelAttribute(name = "定价")
	private String price; // 定 价
	@ExcelAttribute(name = "存储位置")
	private String savePlace; // 存储位置
	@ExcelAttribute(name = "数据下载权限")
	private String downPermission; // 数据下载权限
	@ExcelAttribute(name = "备注")
	private String remarks; // 备 注
	@ExcelAttribute(name = "流转意见")
	private String flowSuggestion; // 流转意见
	@ExcelAttribute(name = "资料状态")
	private String fileStatus; // 资料状态
	@ExcelAttribute(name = "资料总数")
	private String total; // 资料总数
	@ExcelAttribute(name = "可借数量")
	private String canBorrowing; // 可借数量
	@Column(name = "status", length = 32)
	private String status; // 审核状态

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getBookName() {
		return bookName;
	}

	public void setBookName(String bookName) {
		this.bookName = bookName;
	}

	public String getOneSubClass() {
		return oneSubClass;
	}

	public void setOneSubClass(String oneSubClass) {
		this.oneSubClass = oneSubClass;
	}

	public String getTwoSubClass() {
		return twoSubClass;
	}

	public void setTwoSubClass(String twoSubClass) {
		this.twoSubClass = twoSubClass;
	}

	public String getInventoryNum() {
		return inventoryNum;
	}

	public void setInventoryNum(String inventoryNum) {
		this.inventoryNum = inventoryNum;
	}

	public Date getPublishDate() {
		return publishDate;
	}

	public void setPublishDate(Date publishDate) {
		if (publishDate != null) {
			try {
				String date = new SimpleDateFormat("yyyy-MM-dd")
						.format(publishDate);
				publishDate = new SimpleDateFormat("yyyy-MM").parse(date);
			} catch (ParseException e) {
				e.printStackTrace();
			}
		}
		this.publishDate = publishDate;
	}

	public String getVersion() {
		return version;
	}

	public void setVersion(String version) {
		this.version = version;
	}

	public String getPublishUnit() {
		return publishUnit;
	}

	public void setPublishUnit(String publishUnit) {
		this.publishUnit = publishUnit;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public String getEntry() {
		return entry;
	}

	public void setEntry(String entry) {
		this.entry = entry;
	}

	public String getReviewers() {
		return reviewers;
	}

	public void setReviewers(String reviewers) {
		this.reviewers = reviewers;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}

	public Date getPrintDate() {
		return printDate;
	}

	public void setPrintDate(Date printDate) {
		if (printDate != null) {
			try {
				String date = new SimpleDateFormat("yyyy-MM-dd")
						.format(printDate);
				printDate = new SimpleDateFormat("yyyy-MM").parse(date);
			} catch (ParseException e) {
				e.printStackTrace();
			}
		}
		this.printDate = printDate;
	}

	public String getBookNo() {
		return bookNo;
	}

	public void setBookNo(String bookNo) {
		this.bookNo = bookNo;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getSavePlace() {
		return savePlace;
	}

	public void setSavePlace(String savePlace) {
		this.savePlace = savePlace;
	}

	public String getFileStatus() {
		return fileStatus;
	}

	public void setFileStatus(String fileStatus) {
		this.fileStatus = fileStatus;
	}

	public String getDownPermission() {
		return downPermission;
	}

	public void setDownPermission(String downPermission) {
		this.downPermission = downPermission;
	}

	public String getRemarks() {
		return remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}

	public String getFlowSuggestion() {
		return flowSuggestion;
	}

	public void setFlowSuggestion(String flowSuggestion) {
		this.flowSuggestion = flowSuggestion;
	}

	public String getTotal() {
		return total;
	}

	public String getCanBorrowing() {
		return canBorrowing;
	}

	public void setTotal(String total) {
		this.total = total;
	}

	public void setCanBorrowing(String canBorrowing) {
		this.canBorrowing = canBorrowing;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}
}
