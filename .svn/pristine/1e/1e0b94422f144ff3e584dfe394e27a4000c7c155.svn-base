<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.background.organization.organization.OrganizationUsersRelation" table="ORGANIZATION_USERS_RELATION">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="orgId" type="java.lang.String">
            <column name="org_id" />
        </property>
        <property name="userId" type="java.lang.String">
            <column name="user_id" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="date">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="date">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getOrganizationUsersRelation">
    	<![CDATA[ from OrganizationUsersRelation]]>
    </query>
    <query name="getOrganizationUsersRelationById">
    	<![CDATA[ from OrganizationUsersRelation where id= :id]]> 
    </query>
    <query name="getUsersByOrgId">
    	<![CDATA[ from OrganizationUsersRelation where orgId= :orgId]]>
    </query>
     <query name="getUsersByUserId">
    	<![CDATA[ from OrganizationUsersRelation where userId= :userId]]>
    </query>
</hibernate-mapping>
