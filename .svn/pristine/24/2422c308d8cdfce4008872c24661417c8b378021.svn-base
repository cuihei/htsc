package com.ht.persistence.model.system.workflow.process;

import com.ht.persistence.model.base.BaseModel;

/**
 * 海图流程任务实体
 * @author 王有为
 * @date 2016/10/17
 */
public class HtTask extends BaseModel{
	
	/**
	 * 流程定义ID
	 */
	private String processDefId = null;
	
	/**
	 * 流程定义名称
	 */
	private String processDefName = null;
	
	/**
	 * 任务定义ID
	 */
	private String taskDefId = null;
	
	/**
	 * 任务定义名称
	 */
	private String taskDefName = null;

	public String getProcessDefId() {
		return processDefId;
	}

	public void setProcessDefId(String processDefId) {
		this.processDefId = processDefId;
	}

	public String getProcessDefName() {
		return processDefName;
	}

	public void setProcessDefName(String processDefName) {
		this.processDefName = processDefName;
	}

	public String getTaskDefId() {
		return taskDefId;
	}

	public void setTaskDefId(String taskDefId) {
		this.taskDefId = taskDefId;
	}

	public String getTaskDefName() {
		return taskDefName;
	}

	public void setTaskDefName(String taskDefName) {
		this.taskDefName = taskDefName;
	}
}
