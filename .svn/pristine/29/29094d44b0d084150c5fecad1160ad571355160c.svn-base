package com.ht.workflow.service;

import java.util.List;


/**
 * 工作流执行结果数据类
 * @author wyw
 * @version 1.0 2015/07/04
 */
public class WsResult<T> {
	
	/** 执行结果 */
	private String result = null;
	/** 执行结果数据 */
	private T data = null;
	/** 执行结果消息 */
	private String message = null;
	/** 错误项目列表 */
	private List<String> errorItems = null;
	
	/**
	 * 取得执行结果
	 * @return 执行结果
	 */
	public String getResult() {
		
		// 返回执行结果
		return result;
	}
	
	/**
	 * 设置执行结果
	 * @param result 执行结果
	 */
	public void setResult(String result) {
		
		// 设置执行结果
		this.result = result;
	}
	
	/**
	 * 取得执行结果数据
	 * @return 执行结果数据
	 */
	public T getData() {
		
		// 返回执行结果数据
		return data;
	}
	
	/**
	 * 设置执行结果数据
	 * @param data 执行结果数据
	 */
	public void setData(T data) {
		
		// 设置执行结果数据
		this.data = data;
	}
	
	/**
	 * 取得执行结果消息
	 * @return 执行结果消息
	 */
	public String getMessage() {
		
		// 返回执行结果消息
		return message;
	}
	
	/**
	 * 设置执行结果消息
	 * @param messages 执行结果消息
	 */
	public void setMessage(String message) {
		
		// 设置执行结果消息
		this.message = message;
	}
	
	/**
	 * 取得错误项目列表
	 * @return 错误项目列表
	 */
	public List<String> getErrorItems() {
		
		// 返回错误项目列表
		return errorItems;
	}
	
	/**
	 * 设置错误项目列表
	 * @param errorItems 错误项目列表
	 */
	public void setErrorItems(List<String> errorItems) {
		
		// 设置错误项目列表
		this.errorItems = errorItems;
	}
}
