<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.system.symbol.Symbol" table="Symbol">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="pictureType" type="java.lang.String">
            <column name="picture_type" />
        </property>
        <property name="imgUrl" type="java.lang.String">
            <column name="img_url" />
        </property>
        <property name="code" type="java.lang.String">
            <column name="symbol_code" />
        </property>
        <property name="img" type="java.lang.String">
            <column name="img" />
        </property>
         <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        
    </class>
    <query name="getSymbolList">
    	<![CDATA[ from Symbol]]>
    </query>
    
    <query name="getSymbolsListById">
    	<![CDATA[ from Symbol where id =:id]]>
    </query>
    <query name="getSymbolsListByCode">
    	<![CDATA[ from Symbol where code =:code]]>
    </query>
</hibernate-mapping>
