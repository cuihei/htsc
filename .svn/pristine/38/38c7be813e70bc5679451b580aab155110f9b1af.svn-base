package com.ht.persistence.model.drawtask.taskbook.history;


import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import com.ht.persistence.model.base.BaseModel;

/**
 * 编绘任务书model
 * @author penghao
 */
@Entity
@Table(name = "HISTORY_COMPILATION_TASK_BOOK")
public class HistoryTaskBook extends BaseModel{
 
		@Id
		@Column(name = "id", length = 64)
		private String id;                  //主键
		@Column(name = "taskbook_name", length = 100, nullable = false)
		private String taskbookName;        //任务书名称
		@Column(name = "taskbook_no", length = 64, nullable = false)
		private String taskbookId;			//任务书id
		private String taskbookNo;			//任务书序号
		private String taskName;			//任务名称
		private String taskFrom;			//任务来源
		private String executeDeptId;		//执行部门
		private Date executeTime;			//执行时间
		private String technologyStandard;	//技术标准
		private String technologyDemand;	//技术要求
		private String otherDemand;			//其它要求
		private String revision;			//版次
		private String authenticity;		//是否真实
		private String enclosure;			//附件
		private String taskBookType;		//任务书类型
		private String projectName;			//工程名称
		private String noticeNo;			//通告期号
		private String correctNo;			//改正期号
		private String correctNoticeNo;		//改正通告期号
		private String itemName;			//项目名称
		private Date issueTime;			//下发时间
		public String getId() {
			return id;
		}
		public void setId(String id) {
			this.id = id;
		}
		public String getTaskbookName() {
			return taskbookName;
		}
		public void setTaskbookName(String taskbookName) {
			this.taskbookName = taskbookName;
		}
		public String getTaskbookNo() {
			return taskbookNo;
		}
		public void setTaskbookNo(String taskbookNo) {
			this.taskbookNo = taskbookNo;
		}
		
		public String getTaskName() {
			return taskName;
		}
		public void setTaskName(String taskName) {
			this.taskName = taskName;
		}
		public String getTaskFrom() {
			return taskFrom;
		}
		public void setTaskFrom(String taskFrom) {
			this.taskFrom = taskFrom;
		}
		public String getExecuteDeptId() {
			return executeDeptId;
		}
		public void setExecuteDeptId(String executeDeptId) {
			this.executeDeptId = executeDeptId;
		}
		public Date getExecuteTime() {
			return executeTime;
		}
		public void setExecuteTime(Date executeTime) {
			this.executeTime = executeTime;
		}
		public String getTechnologyStandard() {
			return technologyStandard;
		}
		public void setTechnologyStandard(String technologyStandard) {
			this.technologyStandard = technologyStandard;
		}
		public String getTechnologyDemand() {
			return technologyDemand;
		}
		public void setTechnologyDemand(String technologyDemand) {
			this.technologyDemand = technologyDemand;
		}
		public String getOtherDemand() {
			return otherDemand;
		}
		public void setOtherDemand(String otherDemand) {
			this.otherDemand = otherDemand;
		}
		public String getRevision() {
			return revision;
		}
		public void setRevision(String revision) {
			this.revision = revision;
		}
		public String getAuthenticity() {
			return authenticity;
		}
		public void setAuthenticity(String authenticity) {
			this.authenticity = authenticity;
		}
		public String getEnclosure() {
			return enclosure;
		}
		public void setEnclosure(String enclosure) {
			this.enclosure = enclosure;
		}
		public String getTaskbookId() {
			return taskbookId;
		}
		public void setTaskbookId(String taskbookId) {
			this.taskbookId = taskbookId;
		}
		public String getTaskBookType() {
			return taskBookType;
		}
		public void setTaskBookType(String taskBookType) {
			this.taskBookType = taskBookType;
		}
		
		public String getProjectName() {
			return projectName;
		}
		public void setProjectName(String projectName) {
			this.projectName = projectName;
		}
		public String getNoticeNo() {
			return noticeNo;
		}
		public void setNoticeNo(String noticeNo) {
			this.noticeNo = noticeNo;
		}
		public String getCorrectNo() {
			return correctNo;
		}
		public void setCorrectNo(String correctNo) {
			this.correctNo = correctNo;
		}
		public String getCorrectNoticeNo() {
			return correctNoticeNo;
		}
		public void setCorrectNoticeNo(String correctNoticeNo) {
			this.correctNoticeNo = correctNoticeNo;
		}
		public String getItemName() {
			return itemName;
		}
		public void setItemName(String itemName) {
			this.itemName = itemName;
		}
		public Date getIssueTime() {
			return issueTime;
		}
		public void setIssueTime(Date date) {
			this.issueTime = date;
		}
		@Override
		public String toString() {
			return "HistoryTaskBook [id=" + id + ", taskbookName="
					+ taskbookName + ", taskbookId=" + taskbookId
					+ ", taskbookNo=" + taskbookNo + ", taskName=" + taskName
					+ ", taskFrom=" + taskFrom + ", executeDeptId="
					+ executeDeptId + ", executeTime=" + executeTime
					+ ", technologyStandard=" + technologyStandard
					+ ", technologyDemand=" + technologyDemand
					+ ", otherDemand=" + otherDemand + ", revision=" + revision
					+ ", authenticity=" + authenticity + ", enclosure="
					+ enclosure + ", taskBookType=" + taskBookType + "]";
		}
		
}
