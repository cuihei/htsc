<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.datum.datum.Borrowing" table="Borrowing">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="folderId" type="java.lang.String">
            <column name="folder_id" />
        </property>
        <property name="borrowCode" type="java.lang.String">
            <column name="borrow_code" />
        </property>
          <property name="type" type="java.lang.String">
            <column name="type" />
        </property>
        <property name="borrowBookName" type="java.lang.String">
            <column name="borrow_book_name" />
        </property>
        <property name="borrowPerson" type="java.lang.String">
            <column name="borrow_person" />
        </property>
        <property name="borrowDate" type="date">
            <column name="borrow_date" />
        </property>
        <property name="borrowNo" type="java.lang.String">
            <column name="borrow_no" />
        </property>
		<property name="status" type="java.lang.String">
            <column name="status" />
        </property>
        <property name="reNew" type="java.lang.String">
            <column name="renew" />
        </property>
        <property name="renewDays" type="java.lang.String">
            <column name="renew_days" />
        </property>
        <property name="secretInvolved" type="java.lang.String">
            <column name="secret_involved" />
        </property>
      <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getBorrowing">
    	<![CDATA[ from Borrowing]]>
    </query>
    <query name="getBorrowingById">
    	<![CDATA[ from Borrowing where id= :id]]> 
    </query>
    <query name="getBorrowingByFileId">
    	<![CDATA[ from Borrowing where borrowBookNo= :fileId]]> 
    </query>
</hibernate-mapping>
