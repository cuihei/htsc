package com.ht.persistence.model.datum.datum;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Table;

import com.ht.persistence.model.base.BaseModel;

/**
 * 资料借阅实体类
 * @author zyd
 *
 */
@Entity
@Table(name = "V_BORROWING_RETURN_BOOK")
public class VBorrowingReturn extends BaseModel {
	
	private String id; //归还id
	private String borrowId; // 借阅id
	private String borrowCode; //图书编号
	private String jystatus; //借阅状态
	private String ghstatus; //归还状态
	private String borrowBookName; //图书名称
	private String borrowPerson; //借阅人
	private String type; //借阅人
	private String shouldReturn;// 应还数量

	private Date borrowDate; //借阅时间

	private String borrowNo; //借阅数量
	
	private String borrowDays; //借阅天数

	private String returnPerson; //归还人
	private Date returnDate; //归还时间
	private String returnNo; //归还数量
	
	public String getBorrowCode()
	{
		return borrowCode;
	}
	public void setBorrowCode(String borrowCode)
	{
		this.borrowCode = borrowCode;
	}
	public String getBorrowBookName() {
		return borrowBookName;
	}
	public void setBorrowBookName(String borrowBookName) {
		this.borrowBookName = borrowBookName;
	}
	public String getBorrowPerson() {
		return borrowPerson;
	}
	public void setBorrowPerson(String borrowPerson) {
		this.borrowPerson = borrowPerson;
	}
	public Date getBorrowDate() {
		return borrowDate;
	}
	public void setBorrowDate(Date borrowDate) {
		this.borrowDate = borrowDate;
	}
	public String getBorrowNo() {
		return borrowNo;
	}
	public void setBorrowNo(String borrowNo) {
		this.borrowNo = borrowNo;
	}
	public String getBorrowDays() {
		return borrowDays;
	}
	public void setBorrowDays(String borrowDays) {
		this.borrowDays = borrowDays;
	}
	public String getReturnPerson() {
		return returnPerson;
	}
	public void setReturnPerson(String returnPerson) {
		this.returnPerson = returnPerson;
	}
	public Date getReturnDate() {
		return returnDate;
	}
	public void setReturnDate(Date returnDate) {
		this.returnDate = returnDate;
	}
	public String getReturnNo() {
		return returnNo;
	}
	public void setReturnNo(String returnNo) {
		this.returnNo = returnNo;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getJystatus() {
		return jystatus;
	}
	public void setJystatus(String jystatus) {
		this.jystatus = jystatus;
	}
	public String getGhstatus() {
		return ghstatus;
	}
	public void setGhstatus(String ghstatus) {
		this.ghstatus = ghstatus;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getBorrowId()
	{
		return borrowId;
	}
	public void setBorrowId(String borrowId)
	{
		this.borrowId = borrowId;
	}
	public String getShouldReturn()
	{
		return shouldReturn;
	}
	public void setShouldReturn(String shouldReturn)
	{
		this.shouldReturn = shouldReturn;
	}
	
}
