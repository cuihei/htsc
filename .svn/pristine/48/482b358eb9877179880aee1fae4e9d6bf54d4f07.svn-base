package com.ht.workflow.exception;

import com.ht.common.exception.CommonException;

/**
 * 系统工作流异常类
 * @author wyw
 * @version 1.0 2014/09/19
 */
public class WorkflowException extends CommonException {

	/** Serial Version Unique ID */
	private static final long serialVersionUID = 7869615747316841589L;

	/**
	 * 工作流异常类构造方法
	 */
	public WorkflowException() {
		
	}
	
	/**
	 * 工作流异常类构造方法
	 * @param throwable 需要引用的异常
	 */
	public WorkflowException(Throwable throwable) {
		
		// 调用父类构造方法
		super(throwable);
	}
	
	/**
	 * 工作流异常类构造方法
	 * @param messageCode 异常消息编号
	 */
	public WorkflowException(String messageCode) {
		
		// 调用父类构造方法
		super(messageCode);
	}
	
	/**
	 * 工作流异常类构造方法
	 * @param messageCode 异常消息编号
	 * @param arguments
	 */
	public WorkflowException(String messageCode, String[] arguments) {
		
		// 调用父类构造方法
		super(messageCode, arguments);
	}
	
	/**
	 * 工作流异常类构造方法
	 * @param messageCode 异常消息编号
	 * @param detailMessageCode 异常详细消息编号
	 */
	public WorkflowException(String messageCode, String detailMessageCode) {
		
		// 调用父类构造方法
		super(messageCode, detailMessageCode);
	}
	
	/**
	 * 工作流异常类构造方法
	 * @param messageCode 异常消息编号
	 * @param detailMessageCode 异常详细消息编号
	 * @param arguments
	 */
	public WorkflowException(String messageCode, String detailMessageCode, String[] arguments) {
		
		// 调用父类构造方法
		super(messageCode, detailMessageCode, arguments);
	}
}
