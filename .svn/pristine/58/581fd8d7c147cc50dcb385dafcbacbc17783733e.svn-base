$(function(){
	mapsView.init();
})

var mapsView = {
	/**
	 * 初始化
	 */
	init : function(){
		mapsView.bindPageEvent();
	},
	
	/**
	 * 绑定页面事件
	 */
	bindPageEvent : function(){
		/** 绑定返回按钮的click事件*/
		 $("#back").on("click",function(){
			 common.toPage("../maps/index");
			 parent.$("#refresh").click();
		 })

	}
}
