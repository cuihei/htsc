<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.drawtask.plan.VPlan" table="V_PLAN">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="area" type="java.lang.String">
            <column name="area" />
        </property>
        <property name="revision" type="java.lang.String">
            <column name="revision" />
        </property>
        <property name="printQuantity" type="java.lang.String">
            <column name="print_quantity" />
        </property>
        <property name="mapNo" type="java.lang.String">
            <column name="map_no" />
        </property>
        <property name="mapName" type="java.lang.String">
            <column name="map_name" />
        </property>
        <property name="scale" type="java.lang.String">
            <column name="scale" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" />
        </property>
        <property name="workload" type="java.lang.String">
            <column name="workload" />
        </property>
        <property name="deliverTime" type="timestamp">
            <column name="deliver_time" />
        </property>
        <property name="year" type="java.lang.String">
            <column name="year" />
        </property>
        <property name="planMonth" type="java.lang.String">
            <column name="plan_month" />
        </property>
        <property name="relationId" type="java.lang.String">
            <column name="relation_id" />
        </property>
        <property name="measurementPeriod" type="java.lang.String">
            <column name="measurement_period" />
        </property>
        <property name="discription" type="java.lang.String">
            <column name="discription" />
        </property>
         <property name="taskSource" type="java.lang.String">
            <column name="task_source" />
        </property>
         <property name="nature" type="java.lang.String">
            <column name="nature" />
        </property>
        <property name="taskType" type="java.lang.String">
            <column name="task_type" />
        </property>
         <property name="lastYear" type="java.lang.String">
            <column name="last_year" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property> 
        
         <many-to-one name="category" column="category_id"  fetch="select" cascade="none" class="com.ht.persistence.model.background.dicdata.basedata.BaseData" lazy="false" 
        not-found="ignore">
        </many-to-one>
        
    </class>
</hibernate-mapping>
