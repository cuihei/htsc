package com.ht.persistence.model.correctionnotice;

import java.util.Date;

import com.ht.persistence.model.base.BaseModel;
/**
 * 改正通知公告实体类
 * @author huodesheng	
 * @date 2016/10/18
 */
public class CorrectionNotice extends BaseModel {
	//与期表关联id
	private String  id;
	//与期表关联id
	private String  IssueID;
	//项号
	private String ItemNo;
	//创建时间
	private Date CreateTime;
	//标题
	private String Title;
	//关联图号
	private String ChartNo;
	//期号
	private String IssueNo;
	//总期号
	private String TotalIssueNo;
	//发布时间
	private Date ReleaseTime;
	//改正行为
	private String Act;
	//通告类型 
	private String NoticeType;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getIssueID() {
		return IssueID;
	}
	public void setIssueID(String issueID) {
		IssueID = issueID;
	}
	public String getItemNo() {
		return ItemNo;
	}
	public void setItemNo(String itemNo) {
		ItemNo = itemNo;
	}
	public Date getCreateTime() {
		return CreateTime;
	}
	public void setCreateTime(Date createTime) {
		CreateTime = createTime;
	}
	public String getTitle() {
		return Title;
	}
	public void setTitle(String title) {
		Title = title;
	}
	public String getChartNo() {
		return ChartNo;
	}
	public void setChartNo(String chartNo) {
		ChartNo = chartNo;
	}
	public String getIssueNo() {
		return IssueNo;
	}
	public void setIssueNo(String issueNo) {
		IssueNo = issueNo;
	}
	public String getTotalIssueNo() {
		return TotalIssueNo;
	}
	public void setTotalIssueNo(String totalIssueNo) {
		TotalIssueNo = totalIssueNo;
	}
	public Date getReleaseTime() {
		return ReleaseTime;
	}
	public void setReleaseTime(Date releaseTime) {
		ReleaseTime = releaseTime;
	}
	public String getAct() {
		return Act;
	}
	public void setAct(String act) {
		Act = act;
	}
	public String getNoticeType() {
		return NoticeType;
	}
	public void setNoticeType(String noticeType) {
		NoticeType = noticeType;
	}
	
	
	
}
