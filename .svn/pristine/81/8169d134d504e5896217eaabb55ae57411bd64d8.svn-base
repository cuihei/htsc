<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.complication.form.FormContent" table="FORM_CONTENT">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="formId" type="java.lang.String">
            <column name="form_id" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
     <query name="getFormContent">
    	<![CDATA[ from FormContent]]>
    </query>
    <query name="getFormContentByFormId">
    	<![CDATA[ from FormContent where formId= :formId]]> 
    </query>
</hibernate-mapping>
