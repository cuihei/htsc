<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.background.dicdata.scalecontrol.ScaleControl" table="SCALE_CONTROL">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="oldScale" type="java.lang.String">
            <column name="old_scale" />
        </property>
        <property name="standardScale" type="java.lang.String">
            <column name="standard_scale" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getScaleControl">
    	<![CDATA[ from ScaleControl]]>
    </query>
    <query name="getScaleControlById">
    	<![CDATA[ from ScaleControl where id= :id]]> 
    </query>
    <query name="getStandardByOld">
    	<![CDATA[ from ScaleControl where oldScale= :oldScale]]> 
    </query>
</hibernate-mapping>
