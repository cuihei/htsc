<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.complication.formprop.FormFormValue" table="V_FORM_FORM_VALUE">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="processInstId" type="java.lang.String">
            <column name="PROCESS_INST_ID" />
        </property>
        <property name="taskInstId" type="java.lang.String">
            <column name="TASK_INST_ID" />
        </property>
    </class>
    <query name="getFormIdByTTIdAndPIId">
    	<![CDATA[ from FormFormValue where taskInstId = :taskInstId and processInstId = :processInstId]]>
    </query>
</hibernate-mapping>
