<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.statisticalanalysis.CompilationDynamicSummary" table="COMPILATION_DYNAMIC_SUMMARY">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
       <property name="mapNo" type="java.lang.String">
            <column name="map_no" />
        </property>
        <property name="mapName" type="java.lang.String">
            <column name="map_name" />
        </property>
        <property name="scale" type="java.lang.String">
            <column name="scale" />
        </property>
        <property name="property" type="java.lang.String">
            <column name="property" />
        </property>
        <property name="planExchangeTime" type="date">
            <column name="plan_exchange_time" />
        </property>
        <property name="actualExchangeTime" type="date">
            <column name="actual_exchange_time" />
        </property>
        <property name="taskReleaseTime" type="date">
            <column name="task_release_time" />
        </property>
        <property name="planCompleteTime" type="date">
            <column name="plan_complete_time" />
        </property>
        <property name="remarks" type="java.lang.String">
            <column name="remarks" />
        </property>
       	<property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getDynamicTableByTaskBookId">
    	<![CDATA[ from CompilationDynamicSummary where taskBookId = :taskBookId]]>
    </query>
</hibernate-mapping>
