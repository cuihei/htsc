<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.statisticalanalysis.view.CompilationCompleteStatusView" table="V_COMPILATION_COMPLATE_STATUS">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        
        <property name="mapNo" type="java.lang.String">
            <column name="map_no" />
        </property>
        <property name="mapName" type="java.lang.String">
            <column name="map_name" />
        </property>
        <property name="scale" type="java.lang.String">
            <column name="scale" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" />
        </property>
        <property name="completeDate" type="date">
            <column name="complete_date" />
        </property>
         <property name="createYear" type="java.lang.String">
            <column name="create_year" />
        </property>
         <property name="status" type="java.lang.String">
            <column name="status" />
        </property>
    </class>
    <query name="getCompilationCompleteStatus">
    	<![CDATA[ from CompilationCompleteStatusView]]>
    </query>
    <query name="getCompilationCompleteStatusById">
    	<![CDATA[ from CompilationCompleteStatusView where id = :id]]>
    </query>
    <query name="getCompilationCompleteStatusByYear">
    	<![CDATA[ from CompilationCompleteStatusView where createYear = :createYear]]>
    </query>
</hibernate-mapping>
