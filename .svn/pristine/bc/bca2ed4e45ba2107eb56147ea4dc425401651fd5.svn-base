<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.background.organization.organization.Organization" table="ORGANIZATION">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="orgName" type="java.lang.String">
            <column name="org_name" />
        </property>
        <property name="orgNo" type="java.lang.String">
            <column name="org_no" />
        </property>
        <property name="parentId" type="java.lang.String">
            <column name="parent_id" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="date">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="date">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getOrganization">
    	<![CDATA[ from Organization]]>
    </query>
    <query name="getOrganizationById">
    	<![CDATA[ from Organization where id= :id]]> 
    </query>
</hibernate-mapping>
