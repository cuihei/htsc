<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.datum.correctionnoticebook.CorrectionNoticeBookView" table="V_CORRECTION_NOTICE">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="source" type="java.lang.String">
            <column name="source" />
        </property>
        <property name="fileName" type="java.lang.String">
            <column name="file_name" />
        </property>
        <property name="fileId" type="java.lang.String">
            <column name="file_id" />
        </property>
        <property name="titanic" type="java.lang.String">	
            <column name="titanic" />
        </property>
        <property name="receiveDate" type="date">
            <column name="receive_date" />
        </property>
        <property name="content" type="java.lang.String">
            <column name="content" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" />
        </property>
        <property name="state" type="java.lang.String">
            <column name="state" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
      <query name="getCorrectionNoticeBooksByCreateDate">
    	<![CDATA[ from CorrectionNoticeBookView where (creationDate > :beginDate and creationDate < :endDate)  ]]>
    </query>
      <query name="getCorrectionNoticeBooksByCreateDateAndState">
    	<![CDATA[ from CorrectionNoticeBookView where (creationDate > :beginDate and creationDate < :endDate) and state=:state  ]]>
    </query>
    
</hibernate-mapping>
