package com.ht.service.constant.experiencebook.electronicchartsmallcorrection;

import java.util.HashMap;
import java.util.Map;

/**
 * 电子海图小改正经历簿头部常量类
 * @author houchen
 * 2016/12/20
 */
public class EleHead
{
	Map<String,Object> map = new HashMap<String, Object>();
	
	/**
	 * 书签
	 */
	public final String MARKER = "table_dzhtxgzjlb";
	
	/**
	 * 标题
	 */
	final String TITLE = "text_dzhtxgzjlb_title";
	
	/**
	 * 改正期号
	 */
	final String CORRECT_NO = "text_dzhtxgzjlb_correct_no";
	
	/**
	 * 图名
	 */
	final String MAP_NAME = "text_dzhtxgzjlb_map_name";
	
	/**
	 * 图号
	 */
	final String MAP_NO = "text_dzhtxgzjlb_map_no";
	
	/**
	 * 是否需要经过中心质量审定
	 */
	final String YES = "text_dzhtxgzjlb_yes";

	public void setTITLE(String title)
	{
		this.map.put(this.TITLE, title);
	}

	public void setMAP_NAME(String mapName)
	{
		this.map.put(this.MAP_NAME, mapName);
	}
	
	public void setMAP_NO(String mapNo)
	{
		this.map.put(this.MAP_NO, mapNo);
	}
	
	public void setCORRECT_NO(String correctNo)
	{
		this.map.put(this.CORRECT_NO, correctNo);
	}

	public void setYES(String yes)
	{
		this.map.put(this.YES, yes);
	}
	
	public Map<String,Object> getHeadMap(){
		return this.map;
	}
}
