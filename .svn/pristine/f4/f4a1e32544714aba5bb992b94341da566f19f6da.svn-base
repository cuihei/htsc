<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.background.authority.auth.Auth" table="AUTH">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="authNo" type="java.lang.String">
            <column name="auth_no" />
        </property>
        <property name="authName" type="java.lang.String">
            <column name="auth_name" />
        </property>
         <property name="authUrl" type="java.lang.String">
            <column name="auth_url" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getAuth">
    	<![CDATA[ from Auth]]>
    </query>
    <query name="getAuthById">
    	<![CDATA[ from Auth where id= :id]]> 
    </query>
</hibernate-mapping>
