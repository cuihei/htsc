<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.background.dicdata.coefficient.Coefficient" table="COEFFICIENT">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>      
        <property name="mapNo" type="java.lang.String">
            <column name="map_no" />
        </property>
        
        <property name="mapName" type="java.lang.String">
            <column name="map_name" />
        </property>
        
        <property name="scale" type="java.lang.String">
            <column name="scale" />
        </property>
        
        <property name="firstEdition" type="java.lang.String">
            <column name="first_edition" />
        </property>
        
        <property name="reprint" type="java.lang.String">
            <column name="reprint" />
        </property>
        
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        
        <many-to-one name="type" column="type_id"  fetch="select" cascade="none" class="com.ht.persistence.model.background.dicdata.basedata.BaseData" lazy="false" 
        not-found="ignore">
        </many-to-one>
        
    </class>
    <query name="getList">
    	<![CDATA[ from Coefficient]]>
    </query>
    <query name="getCoefficientById">
    	<![CDATA[ from Coefficient where id = :id]]>
    </query>
      <query name="getCoefficientByMapNo">
    	<![CDATA[ from Coefficient where mapNo = :mapNo order by type.id ]]>
    </query>
    
    <query name="getDefect">
    	<![CDATA[ from Coefficient where mapNo = :mapNo and type.id = :typeId]]>
    </query>
</hibernate-mapping>
