<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.datum.bookinfo.ReturnBook" table="RETURN_BOOK">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="borrowId" type="java.lang.String">
            <column name="borrow_id" />
        </property>
        <property name="bookNo" type="java.lang.String">
            <column name="book_no" />
        </property>
        <property name="bookName" type="java.lang.String">
            <column name="book_name" />
        </property>
        
           <property name="concurrentTime" type="date">
            <column name="concurrent_time" />
        </property>
        
        <property name="type" type="java.lang.String">
            <column name="type" />
        </property>
        <property name="returnPerson" type="java.lang.String">
            <column name="return_person" />
        </property>
        <property name="status" type="java.lang.String">
            <column name="status" />
        </property>
        <property name="returnDate" type="date">
            <column name="return_date" />
        </property>
        <property name="returnNo" type="java.lang.String">
            <column name="return_no" />
        </property>
		<property name="borrowDays" type="java.lang.String">
            <column name="borrowDays" />
        </property>
       <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        <property name="curReturnNo" type="java.lang.String">
            <column name="cur_return_no" />
        </property>
    </class>
    
    <query name="getReturnById">
    	<![CDATA[ from ReturnBook where id = :id]]>
    </query>
    
    <query name="getReturnByBorrowId">
    	<![CDATA[ from ReturnBook where borrowId = :borrowId]]>
    </query>
    
    <query name="getReturnBook">
    	<![CDATA[ from ReturnBook]]>
    </query>
</hibernate-mapping>
