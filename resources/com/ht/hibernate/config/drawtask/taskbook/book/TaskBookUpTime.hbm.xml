<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.drawtask.taskbook.book.TaskBookUpTime" table="COMPILATION_TASK_UPTIME">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="state" type="java.lang.String">
            <column name="state" />
        </property>
        <property name="upDate" type="java.lang.String">
            <column name="up_date" />
        </property>
        <property name="upPerson" type="java.lang.String">
            <column name="up_person" />
        </property>
    </class>
  
</hibernate-mapping>
