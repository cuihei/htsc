<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.drawtask.taskbook.history.HistoryTaskBook" table="HISTORY_COMPILATION_TASK_BOOK">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="taskbookId" type="java.lang.String">
            <column name="taskbook_id" />
        </property>
        <property name="taskbookName" type="java.lang.String">
            <column name="taskbook_name" />
        </property>
        <property name="taskbookNo" type="java.lang.String">
            <column name="taskbook_no" />
        </property>
        <property name="taskName" type="java.lang.String">
            <column name="task_name" />
        </property>
        <property name="taskFrom" type="java.lang.String">
            <column name="task_from" />
        </property>
        <property name="executeDeptId" type="java.lang.String">
            <column name="execute_dept_id" />
        </property>
        <property name="executeTime" type="timestamp">
            <column name="execute_time" />
        </property>
        <property name="technologyStandard" type="java.lang.String">
            <column name="technology_standard" />
        </property>
        <property name="technologyDemand" type="java.lang.String">
            <column name="technology_demand" />
        </property>
        <property name="otherDemand" type="java.lang.String">
            <column name="other_demand" />
        </property>
        <property name="revision" type="java.lang.String">
            <column name="revision" />
        </property>
        <property name="authenticity" type="java.lang.String">
            <column name="authenticity" />
        </property>
        <property name="enclosure" type="java.lang.String">
            <column name="enclosure" />
        </property>
         <property name="projectName" type="java.lang.String">
            <column name="project_name" />
        </property>
         <property name="noticeNo" type="java.lang.String">
            <column name="notice_no" />
        </property>
         <property name="correctNo" type="java.lang.String">
            <column name="correct_no" />
        </property>
         <property name="correctNoticeNo" type="java.lang.String">
            <column name="correct_notice_no" />
        </property>
         <property name="itemName" type="java.lang.String">
            <column name="item_name" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        <property name="taskBookType" type="java.lang.String">
            <column name="taskbook_type" />
        </property>
        <property name="issueTime" type="timestamp">
            <column name="ISSUE_TIME" />
        </property>
    </class>
    <query name="findHTaskBookList">
    	<![CDATA[ from HistoryTaskBook order by taskbookNo]]>
    </query>
    <query name="findHTaskBookById">
    	<![CDATA[ from HistoryTaskBook where id= :id order by revision]]> 
    </query>
    <query name="findHTaskBookByTaskBookId">
    	<![CDATA[ from HistoryTaskBook where taskbookId= :id order by issueTime desc]]> 
    </query>
</hibernate-mapping>
