<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.drawtask.taskbook.relation.TaskBookPlanRelation" table="TASKBOOK_PLAN_RELATION">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="taskbookId" type="java.lang.String">
            <column name="taskbook_id" />
        </property>
        <property name="planId" type="java.lang.String">
            <column name="plan_id" />
        </property>
        <property name="revision" type="java.lang.String">
            <column name="revision" />
        </property>
        <property name="year" type="java.lang.String">
            <column name="year" />
        </property>
        <property name="month" type="java.lang.String">
            <column name="month" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
	 <query name="findTaskBookPlanReleations">
    	<![CDATA[ from TaskBookPlanRelation]]>
    </query>
    <query name="findListByBookId">
    	<![CDATA[ from TaskBookPlanRelation where taskbookId= :taskbookId]]> 
    </query>
    <query name="findListByPlanId">
    	<![CDATA[ from TaskBookPlanRelation where planId= :planId]]> 
    </query>
</hibernate-mapping>
