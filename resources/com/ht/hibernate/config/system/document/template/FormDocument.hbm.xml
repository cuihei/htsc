<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.system.document.template.FormDocument" table="FORM_DOCUMENT">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" />
        </property>
        <property name="form_id" type="java.lang.String">
            <column name="form_id" />
        </property>
        <property name="file_id" type="java.lang.String">
            <column name="file_id" />
        </property>
        <property name="files" type="java.sql.Blob">
            <column name="files" />
        </property>
        <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
    </class>
    <query name="getFormDocument">
    	<![CDATA[ from FormDocument]]>
    </query>
    <query name="getFormDocumentById">
    	<![CDATA[ from FormDocument where id= :id]]> 
    </query>
    <query name="getFormDocumentByFormId">
    	<![CDATA[ from FormDocument where form_id= :form_id]]> 
    </query>
</hibernate-mapping>
