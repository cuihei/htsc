<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.system.maps.Maps" table="MAPS">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="mapNo" type="java.lang.String">
            <column name="map_no" />
        </property>
        <property name="picture" type="java.lang.String">
            <column name="picture" />
        </property>
        <property name="pictureType" type="java.lang.String">
            <column name="picture_type" />
        </property>
         <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        
    </class>
    <query name="getMapsList">
    	<![CDATA[ from Maps]]>
    </query>
    
    <query name="getMapsListByMapNo">
    	<![CDATA[ from Maps where id =:id]]>
    </query>
</hibernate-mapping>
