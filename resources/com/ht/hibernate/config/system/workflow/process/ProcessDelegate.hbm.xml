<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.ht.persistence.model.system.workflow.process.ProcessDelegate"
		table="PROCESS_DELEGATE">

		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="groupId" type="java.lang.String">
			<column name="ACT_GROUP_ID" />
		</property>
		<property name="userId" type="java.lang.String">
			<column name="ACT_USER_ID" />
		</property>
		<property name="creator" type="java.lang.String">
			<column name="creator" />
		</property>
		<property name="creationDate" type="timestamp">
			<column name="creation_date" />
		</property>
		<property name="modifier" type="java.lang.String">
			<column name="modifier" />
		</property>
		<property name="modifyDate" type="timestamp">
			<column name="modify_date" />
		</property>
	</class>
	<query name="findList">
    	<![CDATA[ from ProcessDelegate]]>
	</query>
	<query name="findListByGroupId">
    	<![CDATA[ from ProcessDelegate where groupId= :groupId]]>
	</query>
</hibernate-mapping>
