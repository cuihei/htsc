<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ht.persistence.model.system.workflow.process.ProcessForm"
		table="PROCESS_FORM">

		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="formId" type="java.lang.String">
			<column name="FORM_ID" />
		</property>

		<property name="taskDefId" type="java.lang.String">
			<column name="task_def_id" />
		</property>

		<property name="processDefId" type="java.lang.String">
			<column name="PROCESS_DEF_ID" />
		</property>

		<property name="orderNo" type="java.lang.String">
			<column name="ORDER_NO" />
		</property>

		<property name="delegate" type="java.lang.String">
			<column name="DELEGATE" />
		</property>
		<property name="creator" type="java.lang.String">
			<column name="creator" />
		</property>
		<property name="creationDate" type="timestamp">
			<column name="creation_date" />
		</property>
		<property name="modifier" type="java.lang.String">
			<column name="modifier" />
		</property>
		<property name="modifyDate" type="timestamp">
			<column name="modify_date" />
		</property>
	</class>
	<query name="findProcessFormList">
    	<![CDATA[ from ProcessForm]]>
	</query>
	<query name="findProcessFormById">
    	<![CDATA[ from ProcessForm where id= :id]]>
	</query>
	<query name="findProcessFormByProAndTask">
    	<![CDATA[ from ProcessForm where processDefId= :processDefId and taskDefId= :taskDefId]]>
	</query>

	<query name="getProcessFormByProcessDefId">
    	<![CDATA[ from ProcessForm where processDefId= :processDefId order by orderNo]]>
	</query>
</hibernate-mapping>
