<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.system.workflow.process.ProcessLog" table="PROCESS_LOG">

        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        
        <property name="userNo" type="java.lang.String">
            <column name="USER_NO" />
        </property>
        
        <property name="processInstId" type="java.lang.String">
            <column name="PROCESS_INST_ID" />
        </property>
        
        <property name="tableName" type="java.lang.String">
            <column name="TABLE_NAME" />
        </property>
		
	
		 <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        
        <property name="operationType" type="java.lang.String">
            <column name="OPERATION_TYPE" />
        </property>
        
        <property name="tableStatusName" type="java.lang.String">
            <column name="TABLE_STATUS_NAME" />
        </property>
        
        <property name="tableKeyName" type="java.lang.String">
            <column name="TABLE_KEY_NAME" />
        </property>
    </class>
    <query name="findProcessLogList">
    	<![CDATA[ from ProcessLog]]>
    </query>
    <query name="findProcessLogById">
    	<![CDATA[ from ProcessLog where id= :id]]> 
    </query>
    <query name="findProcessLogByProcessInstId">
    	<![CDATA[ from ProcessLog where processInstId= :processInstId]]> 
    </query>
</hibernate-mapping>
