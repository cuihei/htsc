<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ht.persistence.model.system.workflow.process.ProcessLogDetail" table="PROCESS_LOG_DETAIL">
		
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        
        <property name="processLogId" type="java.lang.String">
            <column name="PROCESS_LOG_ID" />
        </property>
        
        <property name="tableName" type="java.lang.String">
            <column name="TABLE_NAME" />
        </property>
        
        <property name="detailRecordId" type="java.lang.String">
            <column name="DETAIL_RECORD_ID" />
        </property>
		 <property name="creator" type="java.lang.String">
            <column name="creator" />
        </property>
        <property name="creationDate" type="timestamp">
            <column name="creation_date" />
        </property>
        <property name="modifier" type="java.lang.String">
            <column name="modifier" />
        </property>
        <property name="modifyDate" type="timestamp">
            <column name="modify_date" />
        </property>
        
 		<property name="processInstId" type="java.lang.String">
            <column name="PROCESS_INST_ID" />
        </property>
    </class>
    <query name="findProcessLogDetailList">
    	<![CDATA[ from ProcessLogDetail]]>
    </query>
    <query name="findProcessLogDetailById">
    	<![CDATA[ from ProcessLogDetail where id= :id]]> 
    </query>
    <query name="getProcessLogDetailListByProcessInstId">
    	<![CDATA[ from ProcessLogDetail where processInstId= :processInstId]]> 
    </query>
    <query name="findProcessLogDetailByLogId">
    	<![CDATA[ from ProcessLogDetail where processLogId= :processLogId]]> 
    </query>
</hibernate-mapping>
